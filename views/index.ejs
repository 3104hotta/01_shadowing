    <a id="download" class="col-md-6 col-md-offset-3 btn btn-primary" onClick="startRecording()">Start</a>
  </div>
  <div class="row">
    <button id="stop" class="col-md-6 col-md-offset-3 btn btn-primary" onClick="stopRecording()">Stop</button>
  </div>
</div>

